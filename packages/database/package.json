{
  "name": "@repo/database",
  "version": "0.0.0",
  "private": true,
  "exports": {
    "./service": "./src/service.ts",
    "./types": "./src/types_db.ts"
  },
  "scripts": {
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "generate": "supabase gen types typescript --project-id \"$SUPABASE_PROJECT_ID\" > src/types_db.ts",
    "seed:accounts": "tsx scripts/seed-accounts.ts",
    "seed:products": "tsx scripts/seed-products.ts",
    "seed": "pnpm seed:accounts && pnpm seed:products"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@repo/config": "workspace:*",
    "@types/node": "^25.0.0",
    "dotenv": "^16.4.5",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
