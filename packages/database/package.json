{
  "name": "@repo/database",
  "version": "0.0.0",
  "private": true,
  "exports": {
    "./service": {
      "types": "./src/service.ts",
      "default": "./src/service.ts"
    },
    "./types": {
      "types": "./src/types_db.ts",
      "default": "./src/types_db.ts"
    }
  },
  "scripts": {
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "generate": "supabase gen types typescript --project-id \"$SUPABASE_PROJECT_ID\" > src/types_db.ts",
    "seed:accounts": "tsx scripts/seed-accounts.ts",
    "seed:products": "tsx scripts/seed-products.ts",
    "seed": "pnpm seed:accounts && pnpm seed:products"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.93.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@repo/config": "workspace:*",
    "@types/node": "^25.2.0",
    "dotenv": "^17.2.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
