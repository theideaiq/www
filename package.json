{
  "name": "www-root",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "turbo format",
    "check": "turbo check",
    "typecheck": "turbo typecheck",
    "test": "turbo test",
    "prepare": "husky",
    "sync": "syncpack list-mismatches",
    "release": "turbo build && changeset publish"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@changesets/cli": "^2.29.8",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "syncpack": "^13.0.4",
    "turbo": "^2.7.4"
  },
  "packageManager": "pnpm@10.27.0",
  "engines": {
    "node": ">=24.12.0",
    "pnpm": ">=9"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,json,css}": [
      "biome check --write --no-errors-on-unmatched"
    ]
  }
}
