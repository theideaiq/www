#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/app/node_modules/.pnpm/next@16.1.1_@babel+core@7.28.5_@playwright+test@1.57.0_babel-plugin-react-compiler@1.0._300f62f52a2d871204e76ea7d7b4412f/node_modules/next/dist/bin/node_modules:/app/node_modules/.pnpm/next@16.1.1_@babel+core@7.28.5_@playwright+test@1.57.0_babel-plugin-react-compiler@1.0._300f62f52a2d871204e76ea7d7b4412f/node_modules/next/dist/node_modules:/app/node_modules/.pnpm/next@16.1.1_@babel+core@7.28.5_@playwright+test@1.57.0_babel-plugin-react-compiler@1.0._300f62f52a2d871204e76ea7d7b4412f/node_modules/next/node_modules:/app/node_modules/.pnpm/next@16.1.1_@babel+core@7.28.5_@playwright+test@1.57.0_babel-plugin-react-compiler@1.0._300f62f52a2d871204e76ea7d7b4412f/node_modules:/app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/app/node_modules/.pnpm/next@16.1.1_@babel+core@7.28.5_@playwright+test@1.57.0_babel-plugin-react-compiler@1.0._300f62f52a2d871204e76ea7d7b4412f/node_modules/next/dist/bin/node_modules:/app/node_modules/.pnpm/next@16.1.1_@babel+core@7.28.5_@playwright+test@1.57.0_babel-plugin-react-compiler@1.0._300f62f52a2d871204e76ea7d7b4412f/node_modules/next/dist/node_modules:/app/node_modules/.pnpm/next@16.1.1_@babel+core@7.28.5_@playwright+test@1.57.0_babel-plugin-react-compiler@1.0._300f62f52a2d871204e76ea7d7b4412f/node_modules/next/node_modules:/app/node_modules/.pnpm/next@16.1.1_@babel+core@7.28.5_@playwright+test@1.57.0_babel-plugin-react-compiler@1.0._300f62f52a2d871204e76ea7d7b4412f/node_modules:/app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../next/dist/bin/next" "$@"
else
  exec node  "$basedir/../next/dist/bin/next" "$@"
fi
